    private void clickedButton1(){
        Button logginButton = (Button) findViewById(R.id.button2);
        logginButton.setOnClickListener(new View.OnClickListener(){
        @Override
        public void onClick(View v{
            Intent Loggin = new Intent(this, tempClass1.class);


    }
    });
        )

        }
    private void clickedSignUp(){
        Button signupButton = (Button) findViewById(R.id.button3);
        signupButton.setOnClickListener(new View.OnClickListener(){
        @Override
        public void onClick(View v){
            Intent Loggin = new Intent(this, temperary1.class);
            startActivity(Loggin);

        }
    });

        }

